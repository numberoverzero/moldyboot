#!/usr/bin/env python
import falcon

from wsgiref import simple_server
from gaas.resources.console import Login

PORT = 8020

api = application = falcon.API()
api.add_route("/login", Login())

httpd = simple_server.make_server("127.0.0.1", PORT, api)
print("Serving CONSOLE on {}".format(PORT))
httpd.serve_forever()
